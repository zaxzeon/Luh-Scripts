Anim.TimePosition = startTime

Anim:AdjustSpeed(1)


delay(1.8, function()

    Anim:Stop()

end)

-- Create VFX circles
local function createCircle()
    local circle = Instance.new("Part")
    circle.Shape = Enum.PartType.Ball
    circle.Size = Vector3.new(1, 1, 1)
    circle.Material = Enum.Material.Neon
    circle.Transparency = 0.5
    circle.Anchored = true
    circle.CanCollide = false
    circle.Parent = workspace
    
    -- Position circle in front of player
    local hrp = character:WaitForChild("HumanoidRootPart")
    circle.CFrame = hrp.CFrame * CFrame.new(0, 0, -3)
    
    -- Expand and fade effect
    local expandTween = game:GetService("TweenService"):Create(circle, 
        TweenInfo.new(0.5), 
        {Size = Vector3.new(5, 5, 5), Transparency = 1}
    )
    expandTween:Play()
    
    -- Clean up after effect
    game:GetService("Debris"):AddItem(circle, 0.5)
end

-- Spawn multiple circles rapidly
for i = 1, 40 do
    delay(i * 0.05, createCircle)
end


    end

end

--[[END OF MOVE 2 ANIM]]

--[[Move 3]]


humanoid.AnimationPlayed:Connect(onAnimationPlayed)


local animationId = 10471336737


local player = game.Players.LocalPlayer

local character = player.Character or player.CharacterAdded:Wait()

local humanoid = character:WaitForChild("Humanoid")


local function onAnimationPlayed(animationTrack)

    if animationTrack.Animation.AnimationId == "rbxassetid://" .. animationId then


local p = game.Players.LocalPlayer

local Humanoid = p.Character:WaitForChild("Humanoid")


for _, animTrack in pairs(Humanoid:GetPlayingAnimationTracks()) do

    animTrack:Stop()

end


local AnimAnim = Instance.new("Animation")

AnimAnim.AnimationId = "rbxassetid://12463072679"

local Anim = Humanoid:LoadAnimation(AnimAnim)


local startTime = 0.3


Anim:Play()

Anim:AdjustSpeed(0)

Anim.TimePosition = startTime

Anim:AdjustSpeed(1)


delay(1.8, function()

    Anim:Stop()

end)

-- Create the circle effect
local circle = Instance.new("Part")
circle.Shape = Enum.PartType.Cylinder
circle.Size = Vector3.new(0.5, 1, 1)
circle.CFrame = character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0) * CFrame.Angles(0, 0, math.rad(90))
circle.Anchored = true
circle.CanCollide = false
circle.Material = Enum.Material.Neon
circle.Transparency = 0.5
circle.Parent = workspace

-- Animate the circle
local tweenService = game:GetService("TweenService")
local tweenInfo = TweenInfo.new(
    1, -- Time
    Enum.EasingStyle.Quad,
    Enum.EasingDirection.Out
)

local properties = {
    Size = Vector3.new(0.5, 50, 50),
    Transparency = 1
}

local tween = tweenService:Create(circle, tweenInfo, properties)
tween:Play()

-- Clean up
game:GetService("Debris"):AddItem(circle, 2)


    end

end

