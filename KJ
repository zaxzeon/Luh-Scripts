    workspace.Dropkick_SFX:Destroy()
    workspace.Dropkick_Miss:Destroy()
    -- Tm 'speedlines' adndaki paralar bulup silen script
local targetName = "thespeedthingunderultik"

-- Workspace'teki tm objeleri dner
for _, obj in pairs(workspace:GetDescendants()) do
    -- Eer objenin ad 'speedlines' ise
    if obj:IsA("BasePart") and obj.Name == targetName then
        -- Objeyi sil
        obj:Destroy()
    end
end
-- Tm 'speedlines' adndaki paralar bulup silen script
local targetName = "speedlines"

-- Workspace'teki tm objeleri dner
for _, obj in pairs(workspace:GetDescendants()) do
    -- Eer objenin ad 'speedlines' ise
    if obj:IsA("BasePart") and obj.Name == targetName then
        -- Objeyi sil
        obj:Destroy()
    end
end
local targetName = "speedlinesandstuff"

-- Workspace'teki tm objeleri dner
for _, obj in pairs(workspace:GetDescendants()) do
    -- Eer objenin ad 'speedlines' ise
    if obj:IsA("BasePart") and obj.Name == targetName then
        -- Objeyi sil
        obj:Destroy()
    end
end
end)	
    end

end

--[[END OF MOVE 4 ANIM]]

